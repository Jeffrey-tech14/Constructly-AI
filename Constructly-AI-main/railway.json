{
  "services": [
    {
      "name": "frontend",
      "type": "web",
      "buildCommand": "npm run build",
      "startCommand": "npm start",
      "port": 3000,
      "rootDirectory": "."
    },
    {
      "name": "plan-parser-api",
      "type": "web",
      "runtime": "python",
      "buildCommand": "pip install -r requirements.txt",
      "startCommand": "uvicorn main:app --host 0.0.0.0 --port $PORT",
      "port": 8000,
      "rootDirectory": "./elaris-plan-api"
    }
  ]
}
